{
  "name": "meteor-boiler",
  "author": "repetitioestmaterstudiorum",
  "license": "MIT",
  "scripts": {
    "dev": "export $(grep -v '^#' .env | xargs) && meteor --exclude-archs web.browser.legacy",
    "dev:trace": "export $(grep -v '^#' .env | xargs) && NODE_OPTIONS=--trace-warnings meteor --exclude-archs web.browser.legacy",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "lint": "eslint 'imports/**/*.{ts,tsx}'",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "reset": "meteor reset && rimraf package-lock.json && rimraf node_modules/",
    "start": "meteor run"
  },
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "bcrypt": "^5.1.0",
    "lodash": "^4.17.21",
    "meteor-node-stubs": "^1.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/meteor": "^2.9.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  }
}
